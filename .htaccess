AddOutputFilterByType DEFLATE text/html text/css application/javascript

<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresDefault "access plus 7 days"
</IfModule>

<FilesMatch "^\.">
  Order allow,deny
  Deny from all
</FilesMatch>

<Directory "screenshots">
  Require all denied
</Directory>

<Directory "css">
  Require 192.168.0.15
</Directory>
